{
  "description": "Randomly scroll the left nav when slack is idle so you can see when you have PMs waiting",
  "manifest_version": 2,
  "name": "slack-nav-scroll",
  "version": "1.0",
  "homepage_url": "https://github.com/prozacchiwawa/slack-nav-scroll",
  "icons": {
    "96": "icons/scrollify-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.slack.com/*"],
      "js": ["jquery-3.3.1.min.js","idle.js","scrollify.js"]
    }
  ]
}
